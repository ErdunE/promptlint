{
  "name": "@promptlint/domain-classifier",
  "version": "0.4.2",
  "description": "Domain classification engine for PromptLint - intelligent prompt categorization",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run"
  },
  "keywords": [
    "promptlint",
    "domain-classification",
    "ai",
    "prompt-optimization"
  ],
  "author": "PromptLint Team",
  "license": "MIT",
  "dependencies": {
    "@promptlint/shared-types": "file:../shared-types"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  }
}
